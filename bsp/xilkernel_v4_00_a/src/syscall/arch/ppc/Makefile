include ../../../cpu.make

OBJS = mksyscall${CPU_TYPE}.o
INCLUDEDIR = ../../../../../../
INCLUDES = -I$(INCLUDEDIR)/include


all : $(OBJS)

%.o:%.c
	$(CC) $(CFLAGS) -c $< -o $@ $(INCLUDES)

%.o:%.S
	$(CC) $(CFLAGS) -c -D__ASM__ $< -o $@ $(INCLUDES)

clean:
	rm -f $(OBJS) 
